.header {
  font-size: 14px;
  padding: 10rpx 0;  
  display: flex;
  justify-content: space-between;
  overflow-x: hidden;
}
.line {
  width: 100%;
  padding: 5rpx 0;   
}
.text-indent{
  padding-left: 24px;
}
.table {
  border: 0px solid darkgray;
}
.tr,.datePicker {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 3rem;
  align-items: center;
  font-size: 12px;
}
.td {
    width:40%;
    justify-content: center;
    text-align: center;
}
.bg-w{
  background: snow;
}
.bg-g{
  background: #E6F3F9;
}
.th {
  width: 40%;
  justify-content: center;
  background: #3366FF;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
  font-size: 30rpx;
}
.datePicker{
  height: 100%;
  width: 100%;
  font-size: 14px;
}
.datePicker picker {
  color: #fff;
  background-color: #409eff;
  border-color: #409eff;
  border-radius: 4px;
}
.search-btn {
  width: 100%;
  height: 100rpx;
  padding: 0 10rpx;
  line-height: 100rpx;
}
.search-btn1 {
  width: 100%;
  height: 100rpx;
  padding: 0 40rpx;
  line-height: 100rpx;
}
.navigator-hover{
  opacity: .8;
}