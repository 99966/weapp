/*导航部分  */

.section {
  box-sizing: border-box;
  padding: 15rpx 32rpx 0;
}

.flex-wrp {
  margin-top: 9rpx;
  display: flex;
}

.flex-item {
  width: 340rpx;
  height: 180rpx;
  line-height: 180rpx;
  font-size: 26rpx;
}

.w-item1 {
  position: relative;
  align-items: center;
  justify-content: center;
  background-color: #f99135;
  color: #fff;
  margin-right: 9rpx;
  border-radius: 26rpx 0 0 0;
}

.w-item2 {
  position: relative;
  align-items: center;
  justify-content: center;
  background-color: #8c5fe8;
  color: #fff;
  margin-right: 9rpx;
}
.w-item3 {
  position: relative;
  align-items: center;
  justify-content: center;
  background-color: #ffb709;
  color: #fff;
  border-radius: 0 26rpx 0 0;
}
.w-item4 {
  position: relative;
  align-items: center;
  justify-content: center;
  background-color: #4e9eed;
  color: #fff;
  margin-right: 9rpx;
  border-radius: 0 0 0 26rpx;
}

.w-item5 {
  position: relative;
  align-items: center;
  justify-content: center;
  background-color: #fb5a58;
  color: #fff;
  margin-right: 9rpx;
}
.w-item6 {
  position: relative;
  align-items: center;
  justify-content: center;
  background-color: #1ccdb7;
  color: #fff;
  border-radius: 0 0 26rpx 0;
}

